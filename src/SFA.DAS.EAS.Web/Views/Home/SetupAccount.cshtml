@model SFA.DAS.EAS.Web.OrchestratorResponse<SFA.DAS.EAS.Web.ViewModels.UserAccountsViewModel>

@{ViewBag.PageID = "page-auth-setupaccount"; }
@{ViewBag.Section = "home"; }
@{ViewBag.Title = "Setup your account"; }
@{ViewBag.HideNav = "true"; }


<div class="grid-row">
    <div class="column-full">
        @Html.Partial("_SetupAccount")
    </div>
</div>


@section gaDataLayer {
    <script>
    window.dataLayer = window.dataLayer || []; dataLayer.push({
        'event': 'dataLoaded',
        'loggedIn': '@HttpContext.Current.User.Identity.IsAuthenticated',
        'userID': '',
        'vpv': '/page-auth-setupaccount'
    });
    </script>
}
