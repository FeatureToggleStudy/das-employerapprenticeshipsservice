    <ProjectReference Include="..\SFA.DAS\SFA.DAS.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Scenarios\PayeSchemes.feature.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>PayeSchemes.feature</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Scenarios\PayeSchemes.feature">
      <Generator>SpecFlowSingleFileGenerator</Generator>
      <LastGenOutput>PayeSchemes.feature.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
