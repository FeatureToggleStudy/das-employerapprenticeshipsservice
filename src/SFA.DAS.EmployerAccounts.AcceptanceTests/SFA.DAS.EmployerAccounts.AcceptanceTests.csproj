    <PackageReference Include="NServiceBus" version="7.0.1" />
    <PackageReference Include="SFA.DAS.Configuration" Version="1.0.0.53229" />
    <PackageReference Include="SFA.DAS.NServiceBus" Version="2.0.12" />
    <PackageReference Include="SFA.DAS.Events.Api.Types" version="2.0.0.36524" />
    <PackageReference Include="SFA.DAS.Events.Api.Client" version="1.20.0.26497" />
    <PackageReference Include="SFA.DAS.Sql.Client" version="1.0.0.32930" />
    <PackageReference Include="SpecFlow" Version="2.3.1" />
    <PackageReference Include="SpecFlow.MsTest" Version="2.3.1" />
    <PackageReference Include="System.ValueTuple" version="4.4.0" />
    <PackageReference Include="WindowsAzure.Storage" Version="8.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="app.config">
      <SubType>Designer</SubType>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SFA.DAS.EAS.Web\SFA.DAS.EAS.Web.csproj" />
    <ProjectReference Include="..\SFA.DAS.EmployerAccounts.MessageHandlers\SFA.DAS.EmployerAccounts.MessageHandlers.csproj" />
